{
  "name": "rn-search-and-gallery",
  "version": "1.0.0",
  "description": "React Native application with search functionality and image gallery",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "npx react-native clean && cd ios && xcodebuild clean && cd .. && cd android && ./gradlew clean && cd ..",
    "postinstall": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.75.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-super-grid": "^5.0.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.34.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-elements": "^3.4.3",
    "react-native-paper": "^5.12.3",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-reanimated": "^3.15.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-image-picker": "^7.1.2",
    "react-native-permissions": "^4.1.5",
    "react-native-network-info": "^5.2.1",
    "@react-native-community/netinfo": "^11.3.2",
    "react-native-uuid": "^2.0.2",
    "axios": "^1.7.4",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@types/lodash.debounce": "^4.0.9",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4",
    "@testing-library/react-native": "^12.5.3",
    "@testing-library/jest-native": "^5.4.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8"
  },
  "engines": {
    "node": ">=18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|react-native-vector-icons|react-native-fast-image|react-native-super-grid|react-native-elements|react-native-paper)/)"
    ]
  }
}